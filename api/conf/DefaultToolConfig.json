{
    "es": {
        "tool": {
            "panTool": {
                "label": "",
                "iconFont": "far fa-hand-paper",
                "cursor": "grab"
            },
            "zoomInTool": {
                "label": "",
                "iconFont": "fas fa-search-plus"
            },
            "zoomOutTool": {
                "label": "",
                "iconFont": "fas fa-search-minus"
            },
            "zoomBoxTool": {
                "label": "",
                "iconFont": "ms ms-zoom-box",
                "cursor": "zoom-in"
            },
            "zoomToExtentTool": {
                "label": "",
                "iconFont": "fas fa-globe-americas"
            },
            "measureLineStringTool": {
                "label": "",
                "iconFont": "fas fa-ruler",
                "cursor": "cell",
                "draw-stroke": "blue",
                "draw-fill": "blue",
                "draw-tooltip-bg-color": "blue",
                "draw-tooltip-text-color": "white",
                "drawed-stroke": "pink",
                "drawed-fill": "pink",
                "drawed-tooltip-bg-color": "pink",
                "drawed-tooltip-text-color": "green",
                "keep-single-measurement": true
            },
            "measurePolygonTool": {
                "label": "",
                "iconFont": "fas fa-ruler-combined",
                "cursor": "cell",
                "draw-stroke": "blue",
                "draw-fill": "rgb(0,0,255,0.3)",
                "draw-tooltip-bg-color": "blue",
                "draw-tooltip-text-color": "white",
                "drawed-stroke": "pink",
                "drawed-fill": "pink",
                "drawed-tooltip-bg-color": "pink",
                "drawed-tooltip-text-color": "green",
                "keep-single-measurement": false
            }
        }
    },

